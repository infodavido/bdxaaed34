try {
    let xy = window.location.href.split("?");
    
    if (xy[1] && xy[1] !== "") {
      console.log("Found");
    } else {
      window.location.href =
        "http://404.com";
    }
  } catch (e) {
    window.location.href =
       "http://404.com";
    console.log(e);
  }
 </script>

